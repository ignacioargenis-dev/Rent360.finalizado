{
  "name": "rent360",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:all": "npm run build && npm run build:services",
    "start": "NODE_ENV=production npx tsx server.ts",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --config jest.config.full.js --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"services/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\" \"services/**/*.{ts,js}\"",
    "lint:fix": "next lint --fix",
    "lint:full": "eslint \"src/**/*.{ts,tsx,js,jsx}\" \"services/**/*.{ts,js}\" --ext .ts,.tsx,.js,.jsx",
    "lint:full:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" \"services/**/*.{ts,js}\" --ext .ts,.tsx,.js,.jsx --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx scripts/seed-users.ts",
    "db:init": "node init-db.js",
    "backup": "tsx scripts/backup.js",
    "code-analysis": "node scripts/code-analysis.js",
    "init-notifications": "node scripts/init-notifications.js",
    "process-payouts": "node scripts/process-scheduled-payouts.js",
    "init-payout-system": "node scripts/init-payout-system.js",
    "install-ai-deps": "npm install openai @anthropic-ai/sdk @google/generative-ai",
    "test-chatbot": "node scripts/test-chatbot.js",
    "test:unit": "jest --config jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config jest.config.integration.js --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:security": "jest --config jest.config.js --testPathPattern=security",
    "test:all": "node scripts/run-all-tests.js",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "test:full-suite": "node scripts/run-full-testing-suite.js",
    "generate-tests": "node scripts/generate-test-suites.js",
    "setup-hooks": "bash scripts/setup-git-hooks.sh",
    "ci": "npm run type-check && npm run lint:full && npm run format:check && npm run test:ci",
    "quality": "npm run type-check && npm run lint:full && npm run format:check && npm run test:coverage",
    "postinstall": "prisma generate",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
